{
  "manifest_version": 2,

  "name": "OpenGov Extension by the Sunlight Foundation",
  "description": "This extension will provide users a platform for sending data regarding non-open gov data, dotgov fails, and more, related to open government, accountability, and transparency",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
        "19": "sunlight-foundation-circles-logo-github-icon-transparent-019.png",
        "38": "sunlight-foundation-circles-logo-github-icon-transparent-038.png",
        "64": "sunlight-foundation-circles-logo-github-icon-transparent-064.png",
        "128": "sunlight-foundation-circles-logo-github-icon-transparent-128.png"
    },
   "default_popup": "opengov-button.html",
   "default_title": "Open Up Some Government Data!"
  },
  "permissions": [
   "activeTab",
    "https://ajax.googleapis.com/",
    "tabs"
  ]
}